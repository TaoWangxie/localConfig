            "或",
            "db.getCollection('users').find({})",
            "2.查询特定条件的文档:",
            "db.users.find({'age' : 27})",
            "db.users.find({'username' : 'joe', 'age' : 27})",
            "3.查询排序后的文档:",
            "db.users.find().sort({'age': 1})",
            "4.查询限制数量的文档:",
            "db.users.find().limit(10)",
            "5.查询跳过指定数量的文档:",
            "db.users.find().skip(5)",
            "6.查询并返回特定字段的文档:",
            "db.users.find({}, {'username': 1, '_id': 0})",
